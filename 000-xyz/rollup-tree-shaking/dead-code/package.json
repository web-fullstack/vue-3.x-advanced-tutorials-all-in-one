{
  "name": "dead-code",
  "version": "1.0.0",
  "description": "",
  "main": "src/main.js",
  "scripts": {
    "demo": "npx rollup --config rollup.config.demo.js",
    "start": "npx rollup --config rollup.config.js",
    "iife": "npx rollup --config rollup.config.js ./src/main.js -f iife -o ./dist/iife-main.js",
    "es": "npx rollup --config rollup.config.js ./src/main.js -f es -o ./dist/es-main.js",
    "umd": "npx rollup --config rollup.config.js ./src/main.js -f umd -o ./dist/umd-main.js",
    "prod": "rollup --config rollup.config.prod.js",
    "dev": "rollup --config rollup.config.dev.js",
    "all": "npm run build:*",
    "build-all": "npm run build:iife && npm run build:es && npm run build:umd",
    "build:iife": "rollup --config build/rollup.config.iife.js",
    "build:es": "rollup --config build/rollup.config.es.js",
    "build:umd": "rollup --config build/rollup.config.umd.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "type": "module",
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "rollup": "^2.70.2",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-uglify-es": "^0.0.1",
    "rollup-plugin-vue": "^6.0.0"
  },
  "dependencies": {
    "vue": "^3.2.33"
  }
}
